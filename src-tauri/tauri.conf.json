{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://127.0.0.1:5555",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "notesync",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "identifier": "noteSyncClient1",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "notesync-client-1",
        "width": 800,
        "height": 600,
        "decorations": true,
        "transparent": true,
        "visible": true,
        "focus": false
      }
    ]
  }
}
